{
   "_id": "extrafields:Societe",
   "class": "extrafields",
   "ico": "/img/ico/icSw2/16-Apartment-Building.png",
   "place": [
       [
           [
               "ThirdPartyName",
               "Address",
               "Zip",
               "Town",
               "Country",
               "State",
               "DefaultLang"
           ],
           [
               "Prefix",
               "CustomerCode",
               "SupplierCode",
               "Gencod",
               "Status",
               "Logo"
           ]
       ],
       [
           [
               "Phone",
               "Fax",
               "EMail"
           ],
           [
               "Web",
               "Facebook",
               "LinkedIn"
           ]
       ],
       [
           [
               "ProfId1",
               "ProfId2",
               "ProfId3",
               "ProfId4"
           ],
           [
               "ThirdPartyType",
               "Staff",
               "Capital"
           ]
       ],
       [
           [
               "VATIntra",
               "JuridicalStatus"
           ],
           [
               "VATIsUsed"
           ]
       ]
   ],
   "block": [
       "Main",
       "AddressBook",
       "Deal",
       "Accounting"
   ],
   "longList": [
       "CustomerCode",
       "Zip",
       "Town",
       "Address",
       "Phone",
       "SIREN",
       "SIRET",
       "SalesRepresentatives"
   ],
   "shortList": [
       "Town",
       "Phone",
       "SalesRepresentatives"
   ],
   "fields": {
       "ThirdPartyName": {
           "type": "input",
           "length": 60,
           "enable": true,
           "validate": {
               "required": true,
               "minlength": 3
           }
       },
       "Prefix": {
           "type": "text",
           "length": 5,
           "enable": false
       },
       "CustomerCode": {
           "type": "text",
           "length": 15,
           "enable": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": false
           }
       },
       "SupplierCode": {
           "type": "text",
           "length": 15,
           "enable": false
       },
       "Gencod": {
           "type": "text",
           "enable": false
       },
       "Status": {
           "type": "select",
           "enable": true,
           "default": "ST_NEVER",
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "ST_NO": {
                   "enable": true,
                   "type": "closed",
                   "cssClass": "red-gradient"
               },
               "ST_NEVER": {
                   "enable": true,
                   "type": "never",
                   "cssClass": "grey-gradient"
               },
               "ST_PCOLD": {
                   "enable": true,
                   "type": "prospect",
                   "cssClass": "blue-gradient"
               },
               "ST_PHOT": {
                   "enable": true,
                   "type": "prospect",
                   "cssClass": "red-gradient"
               },
               "ST_CLESS3": {
                   "enable": true,
                   "type": "customer",
                   "cssClass": "orange-gradient"
               },
               "ST_CUST": {
                   "enable": true,
                   "type": "customer",
                   "cssClass": "green-gradient"
               },
               "ST_CPAR": {
                   "enable": false,
                   "type": "customer",
                   "cssClass": "green-gradient"
               }
           }
       },
       "Address": {
           "type": "textarea",
           "cols": 40,
           "rows": 3,
           "enable": true,
           "aoColumns": {
               "sClass": "",
               "bSearchable": true,
               "bVisible": false
           }
       },
       "Zip": {
           "type": "text",
           "length": 15,
           "enable": true,
           "javascript": null,
           "aoColumns": {
               "sClass": "center edit",
               "bSearchable": true,
               "bVisible": false
           }
       },
       "Town": {
           "type": "text",
           "length": 15,
           "enable": true,
           "javascript": null,
           "aoColumns": {
               "sClass": "edit",
               "bSearchable": true,
               "bVisible": true
           }
       },
       "Country": {
           "type": "select",
           "enable": true,
           "dict": "dict:country",
           "default": "substr($langs->defaultlang,3,5)"
       },
       "State": {
           "type": "select",
           "enable": false,
           "dict": null
       },
       "DefaultLang": {
           "type": "select",
           "enable": false,
           "dict": null
       },
       "Logo": {
           "type": "uploadfile",
           "enable": true
       },
       "Phone": {
           "type": "AC_TEL",
           "ico": "AC_TEL",
           "enable": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": false
           },
           "mask": "+99 9 99 99 99 99"
       },
       "Fax": {
           "type": "AC_FAX",
           "ico": "AC_FAX",
           "enable": true,
           "mask": "+99 9 99 99 99 99"
       },
       "EMail": {
           "type": "AC_EMAIL",
           "ico": "AC_EMAIL",
           "enable": true,
           "validate": {
               "email": true
           }
       },
       "Web": {
           "type": "text",
           "ico": "AC_URL",
           "site": "www",
           "enable": true,
           "validate": {
               "url": true
           }
       },
       "Facebook": {
           "type": "text",
           "ico": "AC_URL",
           "site": "facebook",
           "enable": true,
           "validate": {
               "url": true
           }
       },
       "LinkedIn": {
           "type": "text",
           "ico": "AC_URL",
           "site": "linkedin",
           "enable": true,
           "validate": {
               "url": true
           }
       },
       "ProfId1": {
           "type": "text",
           "length": "12",
           "enable": true,
           "aoColumns": {
               "bSearchable": true,
               "bVisible": false
           }
       },
       "ProfId2": {
           "type": "text",
           "length": "25",
           "enable": true,
           "aoColumns": {
               "bSearchable": true,
               "bVisible": false
           }
       },
       "ProfId3": {
           "type": "text",
           "length": "12",
           "enable": true
       },
       "ThirdPartyType": {
           "type": "select",
           "default": "TE_UNKNOWN",
           "enable": true,
           "values": {
               "TE_UNKNOWN": {
                   "enable": true
               },
               "TE_ADMIN": {
                   "enable": true
               },
               "TE_OTHER": {
                   "enable": true
               },
               "TE_FEMME": {
                   "enable": false
               },
               "TE_GROUP": {
                   "enable": true
               },
               "TE_WHOLE": {
                   "enable": false
               },
               "TE_HOMME": {
                   "enable": false
               },
               "TE_PRIVATE": {
                   "enable": true
               },
               "TE_MEDIUM": {
                   "enable": true
               },
               "TE_RETAIL": {
                   "enable": false
               },
               "TE_STARTUP": {
                   "enable": false
               },
               "TE_SMALL": {
                   "enable": true
               }
           }
       },
       "Staff": {
           "type": "select",
           "enable": true,
           "default": "EF0",
           "values": {
               "EF0": {
                   "enable": true,
                   "label": "-"
               },
               "EF1-5": {
                   "enable": true,
                   "label": "1 - 5"
               },
               "EF6-10": {
                   "enable": true,
                   "label": "6 - 10"
               },
               "EF11-50": {
                   "enable": true,
                   "label": "11 - 50"
               },
               "EF51-100": {
                   "enable": true,
                   "label": "51 - 100"
               },
               "EF101-500": {
                   "enable": true,
                   "label": "101 - 500"
               },
               "EF500-": {
                   "enable": true,
                   "label": "> 500"
               }
           }
       },
       "Capital": {
           "type": "text",
           "length": "20",
           "label": "Capital",
           "enable": "1",
           "spinner": "min: 0"
       },
       "VATIsUsed": {
           "type": "checkbox",
           "enable": true,
           "default": true
       },
       "VATIntra": {
           "type": "text",
           "length": 25,
           "javascript": null,
           "enable": true
       },
       "JuridicalStatus": {
           "type": "select",
           "label": "JuridicalStatus",
           "dict": "dict:fk_forme_juridique",
           "enable": true
       }
   }
}