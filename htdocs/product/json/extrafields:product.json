   "ico": "16-Shopping-Cart.png",
   "langs": [
       "products"
   ],
   "defaultprice": "HT",
   "fields": {
       "type": {
           "type": "select",
           "enable": true,
           "default": "PRODUCT",
           "status": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "PRODUCT": {
                   "enable": true,
                   "label": "Product",
                   "cssClass": "blue-gradient"
               },
               "SERVICE": {
                   "label": "Service",
                   "enable": true,
                   "cssClass": "green-gradient"
               }
           }
       },
       "Status": {
           "type": "select",
           "enable": true,
           "default": "DISABLE",
           "status": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "DISABLE": {
                   "enable": true,
                   "label": "NotOnSell",
                   "cssClass": "red-gradient"
               },
               "BUY": {
                   "label": "OnBuy",
                   "enable": true,
                   "cssClass": "orange-gradient"
               },
               "SELL": {
                   "enable": true,
                   "label": "OnSell",
                   "cssClass": "green-gradient"
               },
               "SELLBUY": {
                   "label": "OnBuySell",
                   "enable": true,
                   "cssClass": "blue-gradient"
               }
           }
       },
